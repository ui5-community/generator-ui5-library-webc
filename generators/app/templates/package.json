{
	"name": "<%= libId %>",
	"version": "1.0.0",
	"description": "UI5 Library: <%= libId %> (wrapping <%= webComponentsPackageName %>)",
	"author": "<%= author %>",
	"license": "Apache-2.0",
	"scripts": {
		"clean": "rimraf dist coverage",
		"build": "ui5 build --clean-dest",
		"start": "ui5 serve -o test-resources/<%= libURI %>/kitchen.html",
		"start:dist": "npm start -- --config ui5-dist.yaml",
		"testsuite": "ui5 serve -o test-resources/<%= libURI %>/qunit/testsuite.qunit.html",
		"lint": "eslint src test",
		"karma": "karma start",
		"karma-ci": "karma start karma-ci.conf.js",
		"karma-ci-cov": "karma start karma-ci-cov.conf.js",
		"test": "npm run lint && npm run karma-ci-cov",
		"ui5:prebuild": "rimraf src/<%= libURI %>/thirdparty/ && ui5-webc-prebuild",
		"generate": "ui5-webc-generate"
	},
	"devDependencies": {
		"@ui5/cli": "^3.2.0",
		"@ui5/tooling-webc": "^0.2.9",
		"@ui5/webcomponents": "^1.14.3",
		"@ui5/webcomponents-icons-business-suite": "^1.14.3",
		"@ui5/webcomponents-icons-tnt": "^1.14.3",
		"eslint": "^8.43.0",
		"karma": "^6.4.2",
		"karma-chrome-launcher": "^3.2.0",
		"karma-coverage": "^2.2.0",
		"karma-ui5": "^3.0.3",
		"npm-run-all": "^4.1.5",
		"rimraf": "^5.0.1",
		"typescript": "^5.1.3",
		"ui5-middleware-livereload": "^0.8.3"
	},
	"dependencies": {
		"<%= webComponentsPackageName %>": "<%= webComponentsPackageVersion %>"
	}
}
